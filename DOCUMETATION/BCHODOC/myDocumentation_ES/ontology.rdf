        <rdfs:comment xml:lang="en">Describes the use of buildings</rdfs:comment>
        <rdfs:label xml:lang="es">HB60 Uso del Edificio</rdfs:label>
    </Class>
    


    <!-- http://www.ciudadpatrimoniomundial.com/ontologias/BCH/HB62_BuildingBlock_Usage -->


    <Class rdf:about="http://www.ciudadpatrimoniomundial.com/ontologias/BCH/HB62_BuildingBlock_Usage">

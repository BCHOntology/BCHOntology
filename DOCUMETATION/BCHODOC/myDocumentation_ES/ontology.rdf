        <rdfs:comment xml:lang="en">Stores a person&apos;s socio-economic information</rdfs:comment>
        <rdfs:label xml:lang="es">HB65 Situación Socioeconómica</rdfs:label>
    </Class>
    


    <!-- http://www.ciudadpatrimoniomundial.com/ontologias/BCH/HB66_Construction_Permit_Grant -->


    <Class rdf:about="http://www.ciudadpatrimoniomundial.com/ontologias/BCH/HB66_Construction_Permit_Grant">

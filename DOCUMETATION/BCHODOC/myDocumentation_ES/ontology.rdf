        <rdfs:comment xml:lang="es">Indica si la edificacion es pública o privada</rdfs:comment>
        <rdfs:comment xml:lang="en">Indicates whether the building is public or private</rdfs:comment>
        <rdfs:label xml:lang="es">HB56 Tipo de Dominio</rdfs:label>
    </Class>
    


    <!-- http://www.ciudadpatrimoniomundial.com/ontologias/BCH/HB57_Tenure_Type -->


    <Class rdf:about="http://www.ciudadpatrimoniomundial.com/ontologias/BCH/HB57_Tenure_Type">

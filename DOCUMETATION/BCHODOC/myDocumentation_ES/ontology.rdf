        <rdfs:comment xml:lang="en">Represents physical attributes of a block</rdfs:comment>
        <rdfs:label xml:lang="es">HB60 Característica Física de la Manzana</rdfs:label>
    </Class>
    


    <!-- http://www.ciudadpatrimoniomundial.com/ontologias/BCH/HB61_Building_Usage -->


    <Class rdf:about="http://www.ciudadpatrimoniomundial.com/ontologias/BCH/HB61_Building_Usage">

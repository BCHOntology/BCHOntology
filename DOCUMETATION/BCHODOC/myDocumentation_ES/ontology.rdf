        <rdfs:comment xml:lang="es">Representa atributos decorativos caracteristicos de edificaciones patrimoniales como ornamentos y molduras</rdfs:comment>
        <rdfs:label xml:lang="es">HB55 Superestructura Decorativa</rdfs:label>
    </Class>
    


    <!-- http://www.ciudadpatrimoniomundial.com/ontologias/BCH/HB56_Domain_Type -->


    <Class rdf:about="http://www.ciudadpatrimoniomundial.com/ontologias/BCH/HB56_Domain_Type">

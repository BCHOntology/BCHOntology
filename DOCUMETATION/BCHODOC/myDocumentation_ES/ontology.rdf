        <rdfs:comment xml:lang="en">Represents the architectural style</rdfs:comment>
        <rdfs:label xml:lang="es">HB63 Estilo Arquitectónico</rdfs:label>
    </Class>
    


    <!-- http://www.ciudadpatrimoniomundial.com/ontologias/BCH/HB65_Socio_Economic_Status -->


    <Class rdf:about="http://www.ciudadpatrimoniomundial.com/ontologias/BCH/HB65_Socio_Economic_Status">

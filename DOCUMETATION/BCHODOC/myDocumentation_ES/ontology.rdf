    <!-- http://www.cidoc-crm.org/cidoc-crm/#P8_took_place_on_or_within -->


    <ObjectProperty rdf:about="http://www.cidoc-crm.org/cidoc-crm/#P8_took_place_on_or_within">
        <inverseOf rdf:resource="http://www.cidoc-crm.org/cidoc-crm/#P8i_witnessed"/>

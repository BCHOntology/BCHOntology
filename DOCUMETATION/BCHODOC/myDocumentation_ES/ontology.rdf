        <rdfs:comment xml:lang="es">Almacena la informacion de propietario</rdfs:comment>
        <rdfs:comment xml:lang="en">Stores owner information</rdfs:comment>
        <rdfs:label xml:lang="es">HB57 Tipo de Tenencia</rdfs:label>
    </Class>
    


    <!-- http://www.ciudadpatrimoniomundial.com/ontologias/BCH/HB58_Usage -->


    <Class rdf:about="http://www.ciudadpatrimoniomundial.com/ontologias/BCH/HB58_Usage">
        <rdfs:subClassOf rdf:resource="http://www.cidoc-crm.org/cidoc-crm/#E55_Type"/>
        <rdfs:comment xml:lang="es">Describe el uso en genereal que puede ser de una parcela, edificación, elemento, etc.</rdfs:comment>
        <rdfs:comment xml:lang="en">Describes the general use that can be of a parcel, building, element, etc.</rdfs:comment>
        <rdfs:label xml:lang="es">HB58 Utilización</rdfs:label>
    </Class>
    


    <!-- http://www.ciudadpatrimoniomundial.com/ontologias/BCH/HB59_Usage_Asignment -->


    <Class rdf:about="http://www.ciudadpatrimoniomundial.com/ontologias/BCH/HB59_Usage_Asignment">

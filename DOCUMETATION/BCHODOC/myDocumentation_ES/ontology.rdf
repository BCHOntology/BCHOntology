        <rdfs:comment xml:lang="es">Relacion el uso y el elemento del cual se describe su uso</rdfs:comment>
        <rdfs:comment xml:lang="en">Relates the use and the element whose use is described.</rdfs:comment>
        <rdfs:label xml:lang="es">HB59 Asignación de Uso</rdfs:label>
    </Class>
    


    <!-- http://www.ciudadpatrimoniomundial.com/ontologias/BCH/HB5_Zone -->


    <Class rdf:about="http://www.ciudadpatrimoniomundial.com/ontologias/BCH/HB5_Zone">

        <rdfs:label xml:lang="es">HB61 Uso de la Manzana</rdfs:label>
    </Class>
    


    <!-- http://www.ciudadpatrimoniomundial.com/ontologias/BCH/HB63_Building_Block_Section -->


    <Class rdf:about="http://www.ciudadpatrimoniomundial.com/ontologias/BCH/HB63_Building_Block_Section">
